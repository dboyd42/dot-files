colorscheme peachpuff
set ignorecase
set number
set expandtab
set shiftwidth=4
set shiftround
set softtabstop=4
set smartindent
set tabstop=4
set tw=0
set clipboard+=unnamedplus
set colorcolumn=80
nnoremap <LocalLeader>sv :source $MYVIMRC<CR>
nnoremap <LocalLeader>w :set wrap!<CR>
nnoremap <C-l> :bn<CR>
nnoremap <C-h> :bp<CR>
